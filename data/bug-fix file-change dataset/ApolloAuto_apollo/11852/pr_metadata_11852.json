  "title": "Docker: Fix \"user exists\" test",
  "user": {
    "login": "lemketron",
    "id": 96521,
    "node_id": "MDQ6VXNlcjk2NTIx",
    "avatar_url": "https://avatars.githubusercontent.com/u/96521?v=4",
